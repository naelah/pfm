{
    "builds": [
      {
        "src": "dashboard/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9"
  }
      }
    ],
    "buildCommand":"python manage.py runserver",
    "installCommand": "pip3 install -r requirements.txt,python manage.py collectstatic",

    "routes": [
      {
        "src": "/(.*)",
        "dest": "dashboard/wsgi.py"
      }
    ]
}